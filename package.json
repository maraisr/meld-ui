{
	"name": "meld-ui",
	"version": "0.0.8",
	"description": "The real world OIM library for JavaScript!",
	"main": "dist/Meld.min.js",
	"scripts": {
		"clean": "rm -rf dist/* tmp/*",
		"test": "karma start --single-run",
		"build": "npm run dev && cross-env NODE_ENV=production uglifyjs dist/Meld.js -c -m -o dist/Meld.min.js --stats",
		"dev": "cross-env NODE_ENV=production tsc && rollup -c=rollup.config.js",
		"watch:test": "karma start --auto-watch",
		"watch:dev": "watch 'npm run dev' src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/maraisr/meld-ui.git"
	},
	"keywords": [
		"js",
		"oim"
	],
	"author": "Marais Rossouw",
	"license": "GPL-2.0",
	"bugs": {
		"url": "https://github.com/maraisr/meld-ui/issues"
	},
	"homepage": "https://github.com/maraisr/meld-ui#readme",
	"devDependencies": {
		"babel-polyfill": "^6.5.0",
		"babel-preset-es2015-rollup": "^1.1.1",
		"babel-preset-stage-2": "^6.5.0",
		"chai": "^3.5.0",
		"cross-env": "^1.0.7",
		"karma": "^0.13.21",
		"karma-chai": "^0.1.0",
		"karma-cli": "~0.1.2",
		"karma-mocha": "^0.2.2",
		"karma-phantomjs-launcher": "~1.0.0",
		"mocha": "^2.4.5",
		"phantomjs-prebuilt": "^2.1.4",
		"rollup-plugin-babel": "^2.4.0",
		"rollup-plugin-includepaths": "^0.1.1",
		"uglifyjs": "^2.4.10",
		"watch": "^0.17.1"
	}
}
