{
  "name": "meld-ui",
  "version": "0.0.2",
  "description": "The real world OIM library for JavaScript!",
  "main": "dist/solution.js",
  "scripts": {
    "dev": "tsc && rollup -c=rollup.config.js",
    "build": "npm run dev && uglifyjs dist/Meld.js -c -m -o dist/Meld.min.js --stats",
    "test": "karma start karma.conf.js --log-level debug --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maraisr/meld-ui.git"
  },
  "keywords": [
    "js",
    "oim"
  ],
  "author": "Marais Rossouw",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/maraisr/meld-ui/issues"
  },
  "homepage": "https://github.com/maraisr/meld-ui#readme",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-3": "^6.5.0",
    "karma-cli": "~0.0.4",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "~0.1.4",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-includepaths": "^0.1.1",
    "uglifyjs": "^2.4.10"
  }
}
